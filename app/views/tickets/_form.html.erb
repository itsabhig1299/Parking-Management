<%= form_with scope: ticket, url: tickets_path do |form| %>

	<% if @ticket.errors.any? %>
		<% @ticket.errors.full_messages.each do |message| %>
			<p><%= message %></p>
		<% end %>
	<% end %>

	<!-- <div> -->
		<%#= form.label :time_arrival %>
		<%#= form.text_field :time_arrival %>
	<!-- </div><br> 
	<div> -->
