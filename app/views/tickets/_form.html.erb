		<%#= form.text_field :time_departure %>
	<!-- </div><br>  -->
	<!-- <div>  --><!--
		<%#= form.label :ticket_number %>
		<%#= form.text_field :ticket_number %>
	</div><br> -->
	<div>
		<%= form.label :parking %>
		<%= form.select :parking, parking_options, include_blank: false, required: true %>
	</div><br> 
	<div>
		<%= form.label :registration_number %>
		<%= form.collection_select :car_id, Car.not_in_parking.order(:name), :id, :registration_number, prompt: 'Select Registration No', required: true %>
	</div><br>
